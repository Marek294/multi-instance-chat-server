{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "max-len": ["error", { "code": 120, "ignoreComments": true, "ignoreTemplateLiterals": true }],
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-param-reassign": 0
    }
}
